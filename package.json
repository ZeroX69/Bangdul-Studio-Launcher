{
  "name": "bangdull-studio",
  "version": "1.0.0",
  "description": "BANGDULL STUDIO - Creative AI Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder",
    "release": "electron-builder --publish always"
  },
  "author": "BangDull AI",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "electron-log": "^5.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.7.3",
    "node-machine-id": "^1.1.12",
    "unzipper": "^0.10.0"
  },
  "build": {
    "appId": "com.bangdull.studio",
    "productName": "BangDull Studio",
    "files": [
      "main.js",
      "preload.js",
      "main.html",
      "activation.html",
      "keygen.js",
      "apps.json",
      "package.json",
      "assets/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "github",
      "owner": "ZeroX69",
      "repo": "Bangdul-Studio-Launcher"
    }
  }
}
